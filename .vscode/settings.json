{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "files.associations": {
    "*.prettierignore": "ignore"
  },
  "cSpell.language": "en,de-DE",
  "typescript.tsdk": "./node_modules/typescript/lib",
  "cSpell.words": [
    "bbox",
    "cartesian",
    "friday",
    "greenable",
    "monday",
    "oowv",
    "oowv",
    "saturday",
    "sunday",
    "thursday",
    "tuesday",
    "typia",
    "wednesday",
  ],
  "files.watcherExclude": {
    "**/target": true
  }
}
